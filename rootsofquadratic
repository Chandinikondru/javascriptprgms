# javascriptprgms
function roots(var a,var b,var c)
{
   var det=b*b-4*a*c;
   var res=-b/2*a;
   if(det>0)
   {
      root1=((-b)+Math.sqrt(det))/2*a;
      root2=((-b)-Math.sqrt(det))/2*a;
      return[root1,root2];
   }
   else if(det==0)
   {
      root1=(-b)/2*a;
      return[root1,root1];
    }
    else
    {
      det=-1*det;
      root1=Math.sqrt(det))/2*a;
      return[res+'+'+root1+'i',res+'-'+root1+'i'];
    }
}
